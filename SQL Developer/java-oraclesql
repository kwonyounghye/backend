create table test (
idx number primary key,
contents varchar2(100)
);

create table member (
userid varchar2(50) not null primary key,
passwd varchar2(50) not null,
name varchar2(50) not null,
email varchar2(50),
hp varchar2(50),
zipcode varchar2(5),
address1 varchar2(200),
address2 varchar2(200),
join_date date default sysdate
);

insert into member (userid, passwd, name, email) values ('kim', '1234', '김철수', 'kim@naver.com');
insert into member (userid, passwd, name, email) values ('park', '1234', '박철수', 'park@naver.com');

--로그인 성공
select * from member where userid = 'kim' and passwd = '1234';
--로그인 실패
select * from member where userid = 'kim' and passwd = '2222';
--해야지 sql에 반영됨
commit;

select * from member;

select * from stud;
